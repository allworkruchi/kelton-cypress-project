{
  "name": "kelton-cypress-bdd-pom",
  "version": "1.0.0",
  "description": "Automation project using cypress-bdd-pom-js",
  "main": "index.js",
  "scripts": {
    "test": "cypress open",
    "runOnChrome": "cypress run --browser chrome",
    "run": "cypress run"
  },
  "devDependencies": {
    "cypress": "^9.7.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^1.18.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
